		afficher t[0][i] , t[1][i]
	fin pour

	ecrire "entrez une valeur"		// valeur = x
	lire x

	pour x de t[1][i] faire
		
			afficher ("la factorielle de" , x , "est :" , t[1][x] )
	fin pour

fin




